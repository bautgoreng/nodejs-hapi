{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "no-unused-vars": "off",
        "no-shadow":"off",
        "linebreak-style": ["error", "windows"]
    }
}
